<!DOCTYPE html>
<html>
  <head>
    <title>BaseNew</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="flash_container">
      <% flash.each do |type, notice| %>
        <h3><b><%= notice %></b></h3>
      <% end %>
    </div>
    <%= yield %>
  </body>
</html>
