<h2><%= @airline.name %></h2>

<h3>Unique Adult Passengers</h3>
<div id="airline_adult_passengers">
  <ul>
    <% @airline.unique_adult_passengers.each do |adult_passenger| %>
      <li>
        <%= adult_passenger.name %>, Number of Flights:
        <%= adult_passenger.num_flights %>
      </li>
    <% end %>
  </ul>
</div>